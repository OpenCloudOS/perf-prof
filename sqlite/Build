perf-prof-y += sqlite3.o
CFLAGS_REMOVE_sqlite3.o += $(CFLAGS)
CFLAGS_sqlite3.o += -O3 -g -fvisibility=hidden
CFLAGS_sqlite3.o += -DSQLITE_THREADSAFE=0
CFLAGS_sqlite3.o += -DSQLITE_DEFAULT_MEMSTATUS=0
CFLAGS_sqlite3.o += -DSQLITE_OMIT_DEPRECATED
CFLAGS_sqlite3.o += -DSQLITE_OMIT_PROGRESS_CALLBACK
CFLAGS_sqlite3.o += -DSQLITE_OMIT_SHARED_CACHE
CFLAGS_sqlite3.o += -DSQLITE_DEFAULT_LOCKING_MODE=1 # PRAGMA locking_mode = EXCLUSIVE
CFLAGS_sqlite3.o += -DSQLITE_DEFAULT_SYNCHRONOUS=0  # PRAGMA synchronous = OFF
CFLAGS_sqlite3.o += -DSQLITE_ENABLE_PERCENTILE

perf-prof-y += perf_tp.o

